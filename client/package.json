{
  "name": "samples-tracker",
  "description": "",
  "version": "0.1.0",
  "author": "romgrk",
  "dependencies": {
    "axios": "^0.17.1",
    "classname": "0.0.0",
    "cuid": "^1.3.8",
    "font-awesome": "^4.7.0",
    "is-object": "^1.0.1",
    "node-sass-chokidar": "0.0.3",
    "partial.lenses": "^13.1.1",
    "prop-types": "^15.5.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-drop-zone": "^2.0.0",
    "react-monaco-editor": "^0.12.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-scripts": "1.0.13",
    "recompose": "^0.23.4",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "styled-components": "^2.0.0"
  },
  "devDependencies": {
    "node-plop": "^0.5.5",
    "plop": "^1.7.4",
    "eslint-config-prettier": "^2.5.0",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0",
    "eslint": "^4.7.0",
    "babel-eslint": "^8.0.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-prettier": "^2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "prettier --single-quote --print-width=120 --trailing-comma es5 --no-semi",
    "format": "npm run prettier -- --write '{,!(build|generators)/**/}*.js'",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint **/*.js --quiet",
    "lint-fix": "eslint --fix",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "generate": "plop --plopfile generators/index.js"
  },
  "lint-staged": {
    "{,!(build|generators)/**/}*.js": [
      "npm run prettier -- --write",
      "git add"
    ]
  },
  "engines": {
    "npm": ">=4",
    "node": ">=6"
  },
  "proxy": "http://localhost:3001"
}
